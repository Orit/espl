#include <stdlib.h>
#include <stdio.h>
#include "font.h"
 
 int main(int argc, char **argv) {
   
   int iline;
   int index;
   
   for(iline = 0; iline!=SYMBOL_HEIGHT; ++iline){
     for(index=0; index<argc; index++){
       char *c = argv[iarg];

	printf("%s", alphabet[asc2idx(*c)][iline]);

	}
   }
   
 }
       
     
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
}
 